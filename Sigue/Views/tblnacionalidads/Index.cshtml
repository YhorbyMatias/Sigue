@model IEnumerable<Sigue.Models.tblnacionalidad>

@{
    ViewBag.Title = "Index";
}

<h2>Mantenimiento nacionalidad</h2>

<p>
    @Html.ActionLink("Crear nuevo", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Nacionalidad)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Nacionalidad)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.NacionalidaId }) |
            @Html.ActionLink("Detalles", "Details", new { id=item.NacionalidaId }) |
            @Html.ActionLink("Eliminar", "Delete", new { id=item.NacionalidaId })
        </td>
    </tr>
}

</table>
